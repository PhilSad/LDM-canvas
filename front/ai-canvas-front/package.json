{
  "name": "ai-canvas-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/api": "^4.0.54",
    "@babel/runtime": "^7.19.0",
    "@react-oauth/google": "^0.2.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "aws-amplify": "^4.3.37",
    "env-cmd": "^10.1.0",
    "konva": "^8.3.11",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.1",
    "react-konva-utils": "^0.3.0",
    "react-router-dom": "^6.4.0",
    "react-toastify": "^9.0.8",
    "spinners-react": "^1.0.7",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-local": "env-cmd -f .env.loc react-scripts start",
    "start-cloud-dev": "env-cmd -f .env.cloud.dev react-scripts start",
    "start-cloud-prod": "env-cmd -f .env.cloud.prod react-scripts start",
    "build": "env-cmd -f .env.cloud react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy-dev": "env-cmd -f .env.cloud.dev react-scripts build && gcloud app deploy --project kollai-front-dev",
    "deploy-prod": "env-cmd -f .env.cloud.dev react-scripts build && gcloud app deploy --project kollai-front-prod"

  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
